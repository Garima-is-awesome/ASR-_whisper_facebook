        "#Loading the Model and Processor\n",
        "#Loads a pre-trained speech-to-text model (openai/whisper-large-v3) from Hugging Face.\n",
        "#The model is loaded onto the GPU if available and optimized to save memory.\n",
        "#Loads the corresponding processor for preprocessing audio input.\n",
        "import torch\n",
        "from transformers import AutoModelForSpeechSeq2Seq, AutoProcessor, pipeline\n",
        "from datasets import load_dataset\n",
        "\n",
        "\n",
        "device = \"cuda:0\" if torch.cuda.is_available() else \"cpu\"\n",
        "torch_dtype = torch.float16 if torch.cuda.is_available() else torch.float32\n",
        "\n",
        "model_id = \"openai/whisper-large-v3\"\n",
        "\n",
        "model = AutoModelForSpeechSeq2Seq.from_pretrained(\n",
        "    model_id, torch_dtype=torch_dtype, low_cpu_mem_usage=True, use_safetensors=True\n",
        ")\n",
        "model.to(device)\n",
        "\n",
        "processor = AutoProcessor.from_pretrained(model_id)\n",
        "\n",
        "# Pipeline for Automatic Speech Recognition (ASR)\n",
        "#Sets up a pipeline for ASR using the model and processor. It supports tokenizing and extracting features from audio, chunking the input, and returning timestamps of words in the transcription.\n",
        "pipe = pipeline(\n",
        "    \"automatic-speech-recognition\",\n",
        "    model=model,\n",
        "    tokenizer=processor.tokenizer,\n",
        "    feature_extractor=processor.feature_extractor,\n",
        "    max_new_tokens=128,\n",
        "    chunk_length_s=30,\n",
        "    batch_size=16,\n",
        "    return_timestamps=True,\n",
        "    torch_dtype=torch_dtype,\n",
        "    device=device,\n",
        ")\n",
        "\n",
        "#Loading the Dataset and Running ASR\n",
        "#Loads a speech dataset from Hugging Face's datasets library.\n",
        "#Applies the pipeline to an audio file (/content/nepali.mpeg) to generate text output.\n",
        "dataset = load_dataset(\"distil-whisper/librispeech_long\", \"clean\", split=\"validation\")\n",
        "sample = dataset[0][\"audio\"]\n",
        "\n",
        "result = pipe(\"/content/nepali.mpeg\")\n",
        "print(result[\"text\"])\n"
      ]
