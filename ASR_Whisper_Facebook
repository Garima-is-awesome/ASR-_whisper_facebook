      "cell_type": "code",
      "source": [
        "import tensorflow as tf\n",
        "from datasets import load_dataset\n",
        "\n",
        "# Step 1: Load the dataset (this dataset contains audio data only)\n",
        "dataset = load_dataset(\"distil-whisper/librispeech_long\", \"clean\", split=\"validation\")\n",
        "\n",
        "# Step 2: Preprocess the dataset (just extract the audio arrays)\n",
        "def preprocess_function(batch):\n",
        "    audio = batch[\"audio\"][\"array\"]  # Extracting the audio array\n",
        "    return {\"audio\": audio} # Return a dictionary with the audio data\n",
        "\n",
        "# Apply the preprocessing to the dataset\n",
        "dataset = dataset.map(preprocess_function)\n",
        "\n",
        "# Step 3: Convert the dataset to a TensorFlow Dataset\n",
        "def generator():\n",
        "    for example in dataset:\n",
        "        yield example[\"audio\"] # Access the audio data from the dictionary\n",
        "\n",
        "# Create a TensorFlow dataset with only audio inputs\n",
        "input_shape = [None,]  # Shape of the audio input (audio data varies in length)\n",
        "\n",
        "tf_dataset = tf.data.Dataset.from_generator(\n",
        "    generator,\n",
        "    output_signature=(\n",
        "        tf.TensorSpec(shape=input_shape, dtype=tf.float32),\n",
        "    )\n",
        ")\n",
        "\n",
        "# Step 4: Batch, shuffle, and prepare the dataset for training\n",
        "batch_size = 32\n",
        "tf_dataset = tf_dataset.batch(batch_size).shuffle(buffer_size=1000)\n",
        "\n",
        "# Now 'tf_dataset' contains only the audio data"
      ],
